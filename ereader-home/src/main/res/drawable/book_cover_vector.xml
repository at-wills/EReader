<?xml version="1.0" encoding="UTF-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#f8ebd3" />
        </shape>
    </item>
    <item>
        <rotate
            android:fromDegrees="90"
            android:pivotX="10%"
            android:pivotY="50%"
            android:toDegrees="90">
            <shape android:shape="line">
                <stroke
                    android:width="1dp"
                    android:color="#e1d5bf" />
            </shape>
        </rotate>
    </item>
    <item>
        <rotate
            android:fromDegrees="270"
            android:pivotX="10%"
            android:pivotY="50%"
            android:toDegrees="270">
            <shape android:shape="line">
                <stroke
                    android:width="1dp"
                    android:color="#e1d5bf" />
            </shape>
        </rotate>
    </item>
</layer-list>